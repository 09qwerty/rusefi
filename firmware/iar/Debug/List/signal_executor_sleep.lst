     30          
     31          void scheduleByTime(const char *prefix, scheduling_s *scheduling, efitimeus_t time, schfunc_t callback, void *param) {
     32          	scheduleTask(prefix, scheduling, time - getTimeNowUs(), callback, param);
     33          }
     34          
     35          void scheduleTask(const char *prefix, scheduling_s *scheduling, int delayUs, schfunc_t callback, void *param) {
     36          	int delaySt = MY_US2ST(delayUs);
     37          	if (delaySt <= 0) {
     38          		/**
     39          		 * in case of zero delay, we should invoke the callback
     40          		 */
     41          		callback(param);
     42          		return;
     43          	}
     44          
     45          	lockAnyContext();
     46          	int isArmed = chVTIsArmedI(&scheduling->timer);
     47          	if (isArmed)
     48          		chVTResetI(&scheduling->timer);
     49          
     50          	chVTSetI(&scheduling->timer, delaySt, (vtfunc_t)callback, param);
     51          	unlockAnyContext();
     52          }
